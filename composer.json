{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "beyondcode/laravel-vouchers": "^1.1",
        "bitfumes/laravel-multiauth": "^2.1",
        "cartalyst/converter": "3.0.*",
        "danielme85/laravel-cconverter": "^0.2.1",
        "darryldecode/cart": "~4.0",
        "fideloper/proxy": "^4.0",
        "florianv/laravel-swap": "^2.0",
        "gloudemans/shoppingcart": "^2.6",
        "gocanto/converter": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "~2.1.0",
        "mbezhanov/faker-provider-collection": "^1.2",
        "nyholm/psr7": "^1.1",
        "paypal/rest-api-sdk-php": "*",
        "php-http/curl-client": "^2.0",
        "php-http/message": "^1.7",
        "qoraiche/laravel-mail-editor": "^1.3",
        "safaricom/mpesa": "^1.0",
        "spatie/laravel-permission": "^2.19"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,

    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        }
    ]
}
